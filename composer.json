{
    "name": "gioppy/yaml-content",
    "description": "Laravel package for manage static content saved on YAML file",
    "license": "MIT",
    "authors": [
        {
            "name": "Giovanni Buffa",
            "email": "tech@giovannibuffa.it"
        }
    ],
    "require": {
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "orchestra/testbench": "3.8"
    },
    "autoload": {
        "psr-4": {
            "gioppy\\YamlContent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "gioppy\\YamlContent\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "gioppy\\YamlContent\\Providers\\YamlContentProvider"
            ]
        }
    }
}
